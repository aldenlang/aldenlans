class System
    def __@init__(self)
        self.argv: list[str] = __@sys__.argv
        self.stdin = {
            read: def () -> str,
            readline: def () -> str,
            readlines: def () -> list[str],
        }
        self.stdout = {
            write: def (s: str) -> none,
        }
        self.stderr = {
            write: def (s: str) -> none,
        }
        self.version: str = int
        self.platform: str = int
        self.exit = def (code: int) -> none
    end
end